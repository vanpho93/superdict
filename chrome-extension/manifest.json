{
  "manifest_version": 2,
  "name": "Superdict",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://dictionary.cambridge.org/dictionary/english/**"
      ],
      "js": [
        "jquery.js",
        "data.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
